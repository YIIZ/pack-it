{
  "name": "pack-it",
  "version": "0.0.0",
  "description": "Just pack it.",
  "repository": "m31271n/pack-it",
  "author": {
    "name": "m31271n",
    "email": "m31271n@gmail.com",
    "url": "stack.m31271n.com"
  },
  "bin": "src/cli.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint 'src/**/*.js' 'test/**/*.js'",
    "test": "ava",
    "code-quality": "npm run lint && npm run test"
  },
  "files": [
    "src/"
  ],
  "main": "src/index.js",
  "keywords": [
    "cli",
    ""
  ],
  "dependencies": {
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "eslint": "^4.19.1",
    "eslint-plugin-ava": "^4.5.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-config-prettier": "^2.9.0",
    "prettier": "^1.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4"
  },
  "lint-staged": {
    "linters": {
      "**/*.{json,md}": [
        "prettier --write",
        "git add"
      ],
      "**/*.js": [
        "prettier --write",
        "eslint",
        "git add"
      ]
    }
  },
  "private": true
}
