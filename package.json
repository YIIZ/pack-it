{
  "name": "@teambun/pack-it",
  "version": "0.4.0",
  "description": "Just pack it.",
  "bin": "src/cli.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint 'src/**/*.js'"
  },
  "files": ["src/"],
  "main": "src/index.js",
  "keywords": ["cli", "pack"],
  "dependencies": {
    "archiver": "^2.1.1",
    "fast-glob": "^2.2.2",
    "fs-extra": "^6.0.1",
    "localtime": "^1.0.2",
    "ora": "^2.1.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "prettier": "^1.12.1"
  },
  "lint-staged": {
    "linters": {
      "**/*.{json,md}": ["prettier --write", "git add"],
      "**/*.js": ["prettier --write", "eslint", "git add"]
    }
  },
  "license": "MIT"
}
